# Function to calculate nth Fibonacci number using iteration
def fibonacci_iterative(n):
    if n <= 0:
        return "Invalid input! Enter a positive integer."
    elif n == 1:
        return 0
    elif n == 2:
        return 1
    else:
        a, b = 0, 1
        for i in range(2, n):
            a, b = b, a + b
        return b

# Input from user
n = int(input("Enter the value of n: "))

# Display the result
print(f"The {n}th Fibonacci number is:", fibonacci_iterative(n))
